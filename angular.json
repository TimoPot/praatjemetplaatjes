{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "praatjemetplaatjes": {
      "projectType": "application",
      "schematics": {},
      "root": "",
      "sourceRoot": "projects",
      "prefix": "pmp",
      "architect": {
        "build": {
          "builder": "@angular/build:application",
          "options": {
            "outputPath": "dist/praatjemetplaatjes",
            "index": "projects/pmp/index.html",
            "browser": "projects/pmp/main.ts",
            "polyfills": [],
            "tsConfig": "projects/pmp/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              {
                "glob": "**/*",
                "input": "public"
              }
            ],
            "styles": [
              "node_modules/modern-normalize/modern-normalize.css",
              "projects/pmp/styles.scss"
            ],
            "stylePreprocessorOptions": {
              "includePaths": ["projects/ui/src/styles"]
            },
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kB",
                  "maximumError": "1MB"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kB",
                  "maximumError": "4kB"
                }
              ],
              "outputHashing": "all",
              "serviceWorker": "true"
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular/build:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "praatjemetplaatjes:build:production"
            },
            "development": {
              "buildTarget": "praatjemetplaatjes:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular/build:extract-i18n"
        },
        "test": {
          "builder": "@angular/build:karma",
          "options": {
            "polyfills": [],
            "tsConfig": "tsconfig.spec.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              {
                "glob": "**/*",
                "input": "public"
              }
            ],
            "styles": [
              "node_modules/modern-normalize/modern-normalize.css",
              "projects/pmp/styles.scss"
            ],
            "scripts": []
          }
        }
      }
    },
    "ui": {
      "projectType": "library",
      "root": "projects/ui",
      "sourceRoot": "projects/ui/src",
      "prefix": "ui",
      "schematics": {
        "@schematics/angular:component": {}
      },
      "architect": {
        "build": {
          "builder": "@angular/build:ng-packagr",
          "options": {
            "project": "projects/ui/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/ui/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/ui/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular/build:karma",
          "options": {
            "tsConfig": "projects/ui/tsconfig.spec.json",
            "polyfills": []
          }
        }
      }
    }
  },
  "cli": {
    "analytics": "770609bd-f716-4b74-9c31-722c62194bc5"
  },
  "schematics": {
    "@schematics/angular:component": {
      "type": ""
    },
    "@schematics/angular:directive": {
      "type": ""
    },
    "@schematics/angular:service": {
      "type": ""
    },
    "@schematics/angular:guard": {
      "typeSeparator": "."
    },
    "@schematics/angular:interceptor": {
      "typeSeparator": "."
    },
    "@schematics/angular:module": {
      "typeSeparator": "."
    },
    "@schematics/angular:pipe": {
      "typeSeparator": "."
    },
    "@schematics/angular:resolver": {
      "typeSeparator": "."
    }
  }
}
